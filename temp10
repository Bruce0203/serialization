pub struct Foo {
    a: Bar,
    b: Bar,
    c: Bar,
}

pub struct Bar {
    string: String
}

fn main() { 
    let foo = Foo {
        a: Bar {
	    string: String::from_str("hello").unwrap(),
	},
	b: Bar {
	    string: String::from_str("world").unwrap(),
	},
	c: Bar {
	    string: String::from_str("baz").unwrap(),
	},
    };
}
